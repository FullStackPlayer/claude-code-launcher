{
  "name": "claude-code-launcher",
  "version": "0.9.0",
  "description": "Claude Code launcher with multiple model provider support",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "test": "bun test",
    "build:linux": "bun build --compile --target=bun-linux-x64 --outfile dist/linux/ccl src/index.ts",
    "build:macos:arm": "bun build --compile --target=bun-darwin-arm64 --outfile dist/macos/arm/ccl src/index.ts",
    "build:macos:x64": "bun build --compile --target=bun-darwin-x64 --outfile dist/macos/x64/ccl src/index.ts",
    "build:windows": "bun build --compile --target=bun-windows-x64 --outfile dist/windows/ccl.exe src/index.ts",
    "build:all": "mkdir -p dist && bun run build:linux && bun run build:macos:arm && bun run build:macos:x64 && bun run build:windows",
    "postinstall": "mkdir -p bin",
    "postbuild:linux": "chmod +x dist/linux/ccl",
    "postbuild:macos:arm": "chmod +x dist/macos/arm/ccl",
    "postbuild:macos:x64": "chmod +x dist/macos/x64/ccl",
    "postbuild:windows": "echo 'Windows executable created'"
  },
  "keywords": [
    "claude-code",
    "anthropic",
    "ai",
    "coding-assistant",
    "launcher"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.6.0",
    "prompts": "^2.4.2"
  }
}